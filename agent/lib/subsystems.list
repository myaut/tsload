# List of subsystems provided by various TSLoad libraries
# 
# parameters:
# 		lib - library that implements that subsystem
# 		cmd - command that contains subsystem
#		alias - pretty name for a subsystem
# 		deps - comma separated list of dependences
#		rdeps - comma separated list of reverse dependences (dependants)
#		generic - generic subsystem (subsystems that doesn't have init/fini functions)

[log]
lib=libtscommon

[mempool]
lib=libtscommon

[sched]
lib=libtscommon

[mod]
lib=libtscommon
alias=modules

[nsk]
lib=libtscommon
alias=netsock

[threads]
lib=libtscommon
deps=mempool

[json]
lib=libtsjson
deps=mempool

[tsobj]
lib=libtsobj
deps=json

[tsfile]
lib=libtsfile
deps=mempool,json

[hi_obj]
lib=libhostinfo
alias=hiobject

[randgen]
lib=libtsload
deps=mempool
rdeps=mod

[tpdisp]
lib=libtsload
deps=mempool
rdeps=mod

[rqsched]
lib=libtsload
deps=mempool
rdeps=mod

[wlt]
lib=libtsload
alias=wltype
deps=log,mempool,nsk
rdeps=mod

[wl]
lib=libtsload
deps=log,mempool,rqsched,threads,wlt
alias=workload

[tp]
lib=libtsload
deps=log,mempool,tpdisp,threads
alias=threadpool

[tsload]
lib=libtsload
deps=randgen,wlt,wl,tp,tsobj,hi_obj
generic=True

[tse_error]
cmd=tsexperiment
rdeps=tsobj,tsfile,tsload

[run]
cmd=tsexperiment
deps=wl,tp

[modsrc]
cmd=tsgenmodsrc
deps=mempool